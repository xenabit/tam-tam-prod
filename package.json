{
  "name": "canon",
  "private": true,
  "version": "0.0.0",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && react-snap",
    "prod": "vite preview --port 3001 --open",
    "convert-images": "node convert-images.js",
    "convert-videos": "node convert-videos.js"
  },
  "dependencies": {
    "@gsap/react": "^2.1.2",
    "framer-motion": "^11.18.2",
    "gsap": "^3.14.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-intersection-observer": "^9.13.0",
    "react-router-dom": "^7.7.1",
    "react-swipeable": "^7.0.2",
    "swiper": "^11.2.1",
    "vite-plugin-svgr": "^4.3.0"
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "eslint-plugin-unused-imports": "^4.1.4",
    "fluent-ffmpeg": "^2.1.3",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",
    "prettier-plugin-css-order": "^2.1.2",
    "react-snap": "^1.23.0",
    "sass": "^1.89.2",
    "sharp": "^0.34.5",
    "vite": "^5.4.8",
    "vite-imagetools": "^7.1.0",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-image-optimizer": "^1.1.9",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-static-copy": "^1.0.0",
    "vitest": "^3.2.4"
  },
  "reactSnap": {
    "source": "dist",
    "minifyHtml": true,
    "inlineCss": false,
    "skipThirdPartyRequests": false,
    "include": [
      "/",
      "/information",
      "/404.html"
    ],
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--enable-logging",
      "--v=1",
      "--log-level=3"
    ],
    "cacheAjaxRequests": false,
    "crawl": false,
    "waitFor": 10000,
    "fixWebpackChunksIssue": "React",
    "viewport": {
      "width": 1280,
      "height": 720
    }
  },
  "manualChunks": {
    "vendor": [
      "react",
      "react-dom",
      "react-router-dom"
    ],
    "swiper": [
      "swiper/react",
      "swiper/modules"
    ],
    "framerMotion": [
      "framer-motion"
    ],
    "styledComponents": [
      "styled-components"
    ],
    "slider": [
      "@/components/Slider/Slider",
      "@/components/SliderScroll/SliderScroll"
    ],
    "hexagrams": [
      "@/components/Hexagrams/Hexagrams"
    ]
  }
}
